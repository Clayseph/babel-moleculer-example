{
  "name": "babel-moleculer-example",
  "version": "1.0.0",
  "description": "My Moleculer microservices project",
  "scripts": {
    "build": "babel src -d dist --ignore \"**/*.test.js\"",
    "clean": "rm -rf node_modules/ & rm package-lock.json",
    "clean:install": "npm-run-all clean install",
    "ci": "jest --watch",
    "lint": "eslint src tests",
    "start": "nodemon --watch src --exec babel-node node_modules/.bin/moleculer-runner --repl --hot src",
    "test": "jest --coverage"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-add-module-exports": "^1.0.0",
    "eslint": "^5.0.1",
    "jest": "^29.3.1",
    "moleculer-repl": "^0.5.0",
    "nodemon": "^1.18.3",
    "regenerator-runtime": "^0.12.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@google-cloud/pubsub": "^0.24.1",
    "ioredis": "^4.6.2",
    "moleculer": "0.13.7",
    "moleculer-web": "^0.8.0",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
